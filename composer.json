{
    "name": "satoryu/google_suggest",
    "description": "This package provides a client to fetch suggest words to given phrases from Google",
    "type": "library",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Tatsuya Sato",
            "email": "satoryu.1981@gmail.com",
            "homepage": "https://satoryu.github.io/",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/satoryu/google_suggest-php",
    "readme": "README.md",
    "support": {
